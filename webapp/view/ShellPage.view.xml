<mvc:View xmlns:cards="sap.f.cards" xmlns:f="sap.f" controllerName="bd.businessportal.controller.ShellPage"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    
    >

    <tnt:ToolPage  
        id="shell_page">

    <tnt:header>
        <Toolbar style="Clear" class="sapUiContentPadding">
            <Button icon="sap-icon://nav-back" press=".navbuttonPressed" type="Transparent"/>
            <Title text="{i18n>title}" />
        </Toolbar>
    </tnt:header>
    <tnt:subHeader>
    </tnt:subHeader>
    <tnt:sideContent>
        <tnt:SideNavigation visible="true" id="sideNavigation" ariaLabel="side panel for navigating to different pages"  expanded="true">
			<tnt:NavigationList id="list"  itemSelect=".onItemSelect" >
				<tnt:NavigationListItem key="Dashboard"   text="Dashboard" icon="sap-icon://home" />
				<tnt:NavigationListItem key="Categories" text="Categories" icon="sap-icon://product" />
				<tnt:NavigationListItem key="Products" text="Products" icon="sap-icon://add-product" />
				<tnt:NavigationListItem key="Orders" text="Orders" icon="sap-icon://clinical-task-tracker" />
				<tnt:NavigationListItem key="Suppliers" text="Suppliers" icon="sap-icon://supplier" />
				<tnt:NavigationListItem key="Shippers" text="Shippers" icon="sap-icon://shipping-status" />

                <tnt:NavigationListItem key="Managers" text="Officials" icon="sap-icon://manager" />
				<tnt:NavigationListItem key="Employees" text="Employees" icon="sap-icon://manager" />
				<tnt:NavigationListItem key="Customers" text="Customers" icon="sap-icon://customer" />
				<tnt:NavigationListItem key="Regions" text="Regions" icon="sap-icon://world" />
				<tnt:NavigationListItem key="Territories" text="Territories" icon="sap-icon://world" />
			</tnt:NavigationList>
			<tnt:fixedItem>
				<tnt:NavigationList>
					<tnt:NavigationListItem key="ImportantLinks" ariaHasPopup="Dialog" id="quickCreate" press=".quickActionPress" text="Important Links" icon="sap-icon://chain-link"  design="Default" selectable="false" />
					<tnt:NavigationListItem key='Legal' selectable="false" href="#" text="Legal"  icon="sap-icon://compare"/>
				</tnt:NavigationList>
			</tnt:fixedItem>
		</tnt:SideNavigation>
        <!-- add content -->
         <!-- <Panel backgroundDesign="Solid" headerText="overview temp" width="auto" class="sapUiTinyMargin">
            <content>
               <Text text="dhfsf"></Text>
            </content>
         </Panel> -->


    </tnt:sideContent>

    <!-- main content -->
     <tnt:mainContents>
       <NavContainer id="navContainer" autoFocus="true"  initialPage="dashboard_page" class="sapUiResponsiveContentPadding" >
        <pages>
            <mvc:XMLView id="dashboard_page" viewName="bd.businessportal.view.Dashboard" />
        </pages>
       </NavContainer>
     </tnt:mainContents>

    </tnt:ToolPage>
    
</mvc:View>